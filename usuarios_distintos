SELECT PROGRESS_RECID
FROM GP.USUARIO u
WHERE 1=1
	AND DT_EXCLUSAO_PLANO > SYSDATE 
	AND CD_CPF = ' '				

UNION 

SELECT COUNT(DISTINCT u.NM_USUARIO)
FROM GP.USUARIO u
JOIN GP.SUSPUSUA s ON s.CD_USUARIO = u.CD_USUARIO
WHERE 1=1
	AND u.DT_EXCLUSAO_PLANO > SYSDATE 	
GROUP BY u.CD_CPF 						
HAVING COUNT(DISTINCT u.NM_USUARIO) > 1	
